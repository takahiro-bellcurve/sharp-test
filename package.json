  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "run-p dev:*",
    "dev:next": "NODE_OPTIONS='--dns-result-order=ipv4first' next dev -p 8292",
    "dev:path": "pathpida --enableStatic --watch",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --fix",
    "format:lint": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' && eslint src --fix",
    "test": "TZ=Asia/Tokyo jest --passWithNoTests",
    "test-update": "jest --updateSnapshot",
    "icons": "npx @svgr/cli -d src/components/icons public/icons",
    "plop": "plop",
    "storybook": "storybook dev -p 6006",
    "storybook-vrt": "storybook dev --config-dir ./.storybook-vrt -p 6006",
    "test-storybook": "test-storybook --excludeTags='vrt'",
    "build-storybook": "storybook build",
    "build-storybook-chromatic": "storybook build --config-dir ./.storybook-vrt --output-dir tmp-chromatic --quiet",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --only-changed --build-command='pnpm build-storybook-chromatic' --output-dir=tmp-chromatic"
